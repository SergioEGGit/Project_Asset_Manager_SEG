//------------------------------Métodos-----------------------------------------

	void VerificarUsuario()
	{
		if(VariablesMetodos::UsuarioA == "Admin")
		{
			cout<< "Menu Administrador" <<endl;
		}
		else
		{
			cout<< "Menu Usuario" <<endl;
		}
	}
